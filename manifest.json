{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.1",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "twitterLogo.png"
    },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://twitter.com/*",
        "https://x.com/*"
      ]
    }
  ]
}
